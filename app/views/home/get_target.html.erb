<h1><%= @drug %> Target List</h1>
<table class="table">
	<thead>
	<tr>
	  <th>#</th>
	  <th>Target</th>
	  <th>Score</th>
	</tr>
	</thead>
	<tbody>
	<% count = 0 %>
	<% @data.each do |key, value| %>
	<tr>
	  <th scope="row"><%= count += 1 %></th>
	  <td><a href="<%= @drug %>/<%= key %>"><%= key %></a></td>
	  <td><%= value %></td>
	</tr>
	<% end %>
	</tbody>
</table>
