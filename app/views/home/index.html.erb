<h1>Drug List</h1>

<div class="row">
  <div class="col-md-6">
  	<h3>Path Model</h3>
	<table class="table">
		<thead>
		<tr>
		  <th>#</th>
		  <th>Target</th>
		</tr>
		</thead>
		<tbody>
		<% count = 0 %>
		<% @path.each do |key| %>
		<tr>
		  <th scope="row"><%= count += 1 %></th>
		  <td><a href="Path/<%= key %>"><%= key %></a></td>
		</tr>
		<% end %>
		</tbody>
	</table>
  </div>
  <div class="col-md-6">
	<h3>ABC Model</h3>
	<table class="table">
		<thead>
		<tr>
		  <th>#</th>
		  <th>Target</th>
		</tr>
		</thead>
		<tbody>
		<% count = 0 %>
		<% @abc.each do |key| %>
		<tr>
		  <th scope="row"><%= count += 1 %></th>
		  <td><a href="ABC/<%= key %>"><%= key %></a></td>
		</tr>
		<% end %>
		</tbody>
	</table>
  </div>
</div>